<!-- Key Codes defined in ncurses.h -->
<!-- Note: Numerical values are in octal -->
<!-- F1 =        0411-->
<!-- F2 =        0412-->
<!-- F3 =        0413-->
<!-- F4 =        0414-->
<!-- F5 =        0415-->
<!-- F6 =        0416-->
<!-- F7 =        0417-->
<!-- F8  =       0420-->
<!-- F9 =        0421-->
<!-- F10 =       0422-->
<!-- ENTER =     012-->
<!-- DEL =       0512-->
<!-- BACKSPC =   0407-->
<!-- ESC  =      033 -->
<!-- UP_ARR =    0403-->
<!-- DOWN_ARR =  0402-->
<!-- RIGHT_ARR = 0405-->
<!-- LEFT_ARR  = 0404-->
<!-- MOUSE_KEY = 0631-->

<!-- KeyDictionaries Format: list of KeyDict -->
 <!-- KeyDict Format: Uuid=key_dict_uuid list of KeyActions -->
  <!-- KeyActions Format: Key=keycode_in_octal list of Actions -->
   <!-- Action Format: object_uuid, method_name -->
  <!-- KeyActions Format -->
 <!-- KeyDict Format -->
<!-- KeyDictionaries Format -->
<KeyDictionaries>
 <KeyDict Uuid="1">
  <KeyActions Key="0412">
   <Action>101, next</Action>
   <Action>102, next</Action>
  </KeyActions>
  <KeyActions Key="033">
   <Action>101, next</Action>
   <Action>102, next</Action>
  </KeyActions>
  <KeyActions Key="012">
   <Action>101, next</Action>
   <Action>102, next</Action>
  </KeyActions>
  <KeyActions Key="0421">
   <Action>101, next</Action>
   <Action>102, next</Action>
  </KeyActions>
  <KeyActions Key="0422">
   <Action>101, next</Action>
   <Action>102, next</Action>
  </KeyActions>
  <KeyActions Key="-1">  <!-- if none of above key is found, these key actions are carried out -->
   <Action>101, stale</Action>
   <Action>102, stale</Action>
  </KeyActions>
 </KeyDict>
 <KeyDict Uuid="2">
  <KeyActions Key="0405">
   <Action>103, next</Action>
   <Action>104, next</Action>
  </KeyActions>
  <KeyActions Key="0404">
   <Action>103, next</Action>
   <Action>104, next</Action>
  </KeyActions>
  <KeyActions Key="-1">
   <Action>103, stale</Action>
   <Action>104, stale</Action>
  </KeyActions>
  <KeyActions Key="033">
   <Action>303, read</Action> <!-- TODO -->
  </KeyActions>
 </KeyDict>
 <KeyDict Uuid="3">
  <KeyActions Key="-1">
   <Action>107, next</Action>
   <Action>108, next</Action>
  </KeyActions>
 </KeyDict>
 <KeyDict Uuid="4">
  <KeyActions Key="0413">
   <Action>106, next</Action>
  </KeyActions>
  <KeyActions Key="033">
   <Action>106, next</Action>
  </KeyActions>
  <KeyActions Key="0414">
   <Action>106, next</Action>
  </KeyActions>
  <KeyActions Key="012">
   <Action>106, next</Action>
  </KeyActions>
  <KeyActions Key="0421">
   <Action>303, markPid</Action>
  </KeyActions>
  <KeyActions Key="-1">
   <Action>106, stale</Action>
  </KeyActions>
 </KeyDict>
 <KeyDict Uuid="5">
  <KeyActions Key="0403">
   <Action>-1, Up</Action>
  </KeyActions>
  <KeyActions Key="0402">
   <Action>-1, Down</Action>
  </KeyActions>
  <KeyActions Key="0631">
   <Action>-1, setSortKey</Action>
   <Action>303, read</Action>
  </KeyActions>
 </KeyDict>
 <!-- TODO: add KeyDict,51 -->
 <!-- TODO: add KeyDict,52 -->
 <KeyDict Uuid="6">
  <KeyActions Key="0403">
   <Action>-1, Up</Action>
  </KeyActions>
  <KeyActions Key="0402">
   <Action>-1, Down</Action>
  </KeyActions>
  <KeyActions Key="012">
   <Action>302, kill</Action> <!-- TODO: add procinfo_uuid -->
   <Action>303, read</Action>
  </KeyActions>
 </KeyDict>
 <KeyDict Uuid="7">
  <KeyActions Key="0403">
   <Action>5, moveLeft</Action>
   <Action>-1, Up</Action>
  </KeyActions>
  <KeyActions Key="0402">
   <Action>5, moveRight</Action>
   <Action>-1, Down</Action>
  </KeyActions>
  <KeyActions Key="012">
   <Action>-1, toggleEditMode</Action>
  </KeyActions>
  <KeyActions Key="0512"> <!-- DON'T CHANGE CALLBACK INVOCATION ORDER -->
   <Action>5, deleteColumn</Action>
   <Action>-1, remove</Action>
  </KeyActions>
 </KeyDict>
 <KeyDict Uuid="8">
  <KeyActions Key="0403">
   <Action>-1, Up</Action>
  </KeyActions>
  <KeyActions Key="0402">
   <Action>-1, Down</Action>
  </KeyActions>
  <KeyActions Key="012">
   <Action>-1, insertIntoLeftNbr</Action>
   <Action>5, insertColumn</Action>
  </KeyActions>
 </KeyDict>
</KeyDictionaries>
